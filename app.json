{
    "name": "YashuAlpha's pyro adder !",
    "description": "based on pyrogram, solo coded in 1hr !",
    "logo": "https://te.legra.ph/file/5d567636cd465b923a005.jpg",
    "repository": "https://github.com/Timeisnotwaiting/pyroadder",
    "keywords": [
        "Alpha-Op"
    ],
    "env": {
        "API_ID": {
            "description": "Get from my.telegram.org/apps",
            "required": true
        },
        "API_HASH": {
            "description": "Get from my.telegram.org/apps",
            "required": true
        },
        "STRING_SESSION": {
            "description": "A valid Pyrogram session string",
            "required": true
        },
        "SUDO_USERS": {
            "description": "split if more than 1",
            "required": true
        },
        "BOT_TOKEN": {
            "description": "GET IT FROM BOTFATHER",
            "required": true
        }
    },
    "formation": {
    "worker": {
      "quantity": 1,
      "size": "free"
    }
  },
    "buildpacks": [
        {
            "url": "heroku/python"
        }
    ]
}
